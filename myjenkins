pipeline {
    agent any

    stages {
        stage('Publish') {
            steps {
                echo 'Publishing HTML Page.............'
                publishHTML([allowMissing: false, alwaysLinkToLastBuild: false, keepAll: false, reportDir: '', reportFiles: 'Default.html', reportName: 'HTML Report', reportTitles: '', useWrapperFileDirectly: true])
            }
        }
    }
}
